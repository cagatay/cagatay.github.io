{% block page %}
<div id="container">
	<header id="page_header" class="clearfix">
    <nav id="menu">
      <ul>
        <li><a href="#">about</a></li>
        <li><a href="#">blog</a></li>
      </ul>
    </nav>
    <div id="profile">
      <a href="/" id="logo"></a>
      <ul id="profile_links">
        <li>
          <a href="http://plus.google.com/100630473385184587136/" class="plus">Google+</a>
        </li>
        <li>
